prefix=C:/Program Files (x86)/MySSH
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libssh2
URL: https://libssh2.org/
Description: Library for SSH-based communication
Version: 1.11.2_DEV
Requires: 
Requires.private: libcrypto
Libs: -L${libdir} -lssh2 
Libs.private: -lcrypto -lcrypt32 -lbcrypt -lz -lws2_32
Cflags: -I${includedir}
